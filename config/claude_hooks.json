        "hooks": [
          {
            "type": "command",
            "command": "bun run C:/Users/kevin/.claude-mem/scripts/context-hook.js"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": ".*",
        "hooks": [
          {
