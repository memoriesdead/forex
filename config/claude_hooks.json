        "matcher": ".*",
        "hooks": [
          {
            "type": "command",
            "command": "bun run C:/Users/kevin/.claude-mem/scripts/user-message-hook.js"
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "matcher": ".*",
        "hooks": [
